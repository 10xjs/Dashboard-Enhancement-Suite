{
  "name": "DES",
  "version": "0.2.0",
  "author": "Neal Granger <neal@nealg.com>",
  "scripts": {
    "clean": "node_modules/.bin/rimraf build && mkdir build",
    "clean:dev": "node_modules/.bin/rimraf dev && mkdir dev",
    "copy": "node_modules/.bin/ncp src/extension build/extension",
    "copy:dev": "node_modules/.bin/ncp src/extension dev/extension",
    "watch": "node_modules/.bin/chokidar src/extension -c \"npm run copy:dev\" & node_modules/.bin/chokidar src/manifest.js -c \"npm run manifest:dev\"",
    "link": "node scripts/link",
    "manifest": "git describe | node scripts/manifest > build/extension/manifest.json",
    "manifest:dev": "git describe | node scripts/manifest > dev/extension/manifest.json",
    "webpack": "node_modules/.bin/webpack src/options/options.js build/extension/options/options.js -p",
    "webpack:dev": "node_modules/.bin/webpack src/options/options.js dev/extension/options/options.js -dw",
    "zip": "node scripts/zip",
    "build": "npm run clean && npm run copy && npm run manifest && npm run webpack && npm run zip",
    "dev": "npm run clean:dev && npm run copy:dev && npm run manifest:dev && npm run webpack:dev & npm run watch"
  },
  "dependencies": {
    "find-index": "^0.1.1",
    "flux": "^2.0.1",
    "keymirror": "^0.1.1",
    "node-jsx": "^0.13.2",
    "object-assign": "^2.0.0",
    "react": "^0.13.2"
  },
  "devDependencies": {
    "archiver": "^0.14.3",
    "chokidar-cli": "^0.2.1",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.13.1",
    "ncp": "^2.0.0",
    "node-zip": "^1.1.1",
    "rimraf": "^2.3.3",
    "webpack": "^1.8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nealgranger/Dashboard-Enhancement-Suite"
  }
}
